{
    "name": "fdoc4chrome",
    "version": "0.1",
    "description": "View fdoc files from Google Chrome",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["*://*/*.fdoc", "file://*/*.fdoc"],
            "js": ["fdoc.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon128.png"
    },
    "icons": {"16": "icon16.png",
              "48": "icon48.png",
              "128": "icon128.png"}
}
